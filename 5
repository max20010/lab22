#include "stdafx.h"
#include <iostream>
#include <fstream>
#include <string>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	string c,str;
	int x, n = 0,spase=0;
	bool ch=false;
	ifstream file("C:\\Users\\Максим\\Desktop\\file.txt");
	while (getline(file, c)) {
		x = c.size();
		str.resize(x, ' ');
		for (int i = 0;i < x;i++) {
			if (c[i] == ' ' && !ch) {
				spase++;
			}
			if (c[i] != ' ') {
				ch = true;
			}
			if (spase >= 5&&!ch) {
				n++;
				ch = true;
			}
		}
		spase = 0;
		ch = false;
	}
	cout << n << endl;
}
